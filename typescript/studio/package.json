{
  "name": "@edgeandnode/amp-studio",
  "description": "UI for interacting with amp datasets. Writing, running and validating queries and datasets to create a dataset definition for deploying",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "start": "vite",
    "build": "vite build && tsc --noEmit",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "check": "prettier --write . && eslint --fix && tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "amp": "amp",
    "cli": "pnpm amp studio"
  },
  "dependencies": {
    "@base-ui/react": "1.2.0",
    "@connectrpc/connect": "^2.1.1",
    "@connectrpc/connect-web": "^2.1.1",
    "@edgeandnode/amp": "workspace:*",
    "@effect-atom/atom": "^0.5.1",
    "@effect-atom/atom-react": "^0.5.0",
    "@graphprotocol/gds-react": "0.2.5",
    "@monaco-editor/react": "4.8.0-rc.2",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-form": "^1.28.3",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router": "^1.160.2",
    "@tanstack/react-router-devtools": "^1.160.2",
    "@tanstack/router-plugin": "^1.160.2",
    "effect": "^3.19.18",
    "monaco-editor": "^0.55.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tanstack/eslint-config": "^0.4.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "jsdom": "^28.1.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "4.0.18",
    "web-vitals": "^5.1.0"
  }
}
