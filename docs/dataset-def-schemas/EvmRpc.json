{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "DatasetDef",
  "type": "object",
  "properties": {
    "kind": {
      "description": "Dataset kind, must be `evm-rpc`.",
      "type": "string"
    },
    "name": {
      "description": "Dataset name.",
      "type": "string"
    },
    "network": {
      "description": "Network name, e.g., `mainnet`.",
      "type": "string"
    },
    "start_block": {
      "description": "Dataset start block.",
      "type": "integer",
      "format": "uint64",
      "default": 0,
      "minimum": 0
    }
  },
  "required": [
    "kind",
    "name",
    "network"
  ]
}