{
  "name": "@edgeandnode/amp",
  "version": "0.0.27",
  "type": "module",
  "license": "UNLICENSED",
  "description": "Nothing to see here yet",
  "repository": {
    "type": "git",
    "url": "https://github.com/edgeandnode/amp-private",
    "directory": "typescript/amp"
  },
  "bin": {
    "amp": "./src/cli/bun.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts",
    "./cli/*": null,
    "./internal/*": null,
    "./*/index": null
  },
  "files": [
    "src/**/*.ts",
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "dist/studio/dist/**/*"
  ],
  "scripts": {
    "build": "tsc -b --force tsconfig.build.json",
    "build:studio": "cd ../studio && pnpm run build",
    "copy-studio-dist": "tsx scripts/copy-studio-dist.ts",
    "prepare-studio": "pnpm run build:studio && pnpm run copy-studio-dist",
    "prepublish": "rm -rf dist && pnpm run build && pnpm run prepare-studio",
    "@edgeandnode/amp": "bun ./src/cli/bun.ts"
  },
  "publishConfig": {
    "bin": {
      "amp": "./dist/cli/main.js"
    },
    "exports": {
      ".": "./dist/index.js",
      "./*": "./dist/*.js",
      "./cli/*": null,
      "./internal/*": null,
      "./*/index": null
    }
  },
  "dependencies": {
    "@effect/cli": "^0.71.0",
    "abitype": "^1.1.1",
    "jiti": "^2.6.1",
    "js-toml": "^1.0.2",
    "minimatch": "^10.0.3",
    "open": "^10.2.0",
    "viem": "^2.38.0"
  },
  "peerDependencies": {
    "@bufbuild/protobuf": "2.6.3",
    "@connectrpc/connect": "^2.1.0",
    "@connectrpc/connect-node": "^2.1.0",
    "@effect/experimental": "^0.56.0",
    "@effect/platform": "^0.92.1",
    "@effect/platform-node": "^0.98.3",
    "apache-arrow": "^21.1.0",
    "effect": "^3.18.4"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.58.0",
    "@bufbuild/protoc-gen-es": "^2.7.0",
    "@connectrpc/connect": "^2.1.0",
    "@connectrpc/connect-node": "^2.1.0",
    "@effect/platform": "^0.92.1",
    "@effect/platform-node": "^0.98.3",
    "@effect/vitest": "^0.26.0",
    "@types/node": "^24.7.2",
    "apache-arrow": "^21.1.0",
    "effect": "^3.18.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
