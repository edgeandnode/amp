{
  "kind": "manifest",
  "network": "mainnet",
  "name": "example",
  "version": "0.1.0",
  "dependencies": {
    "anvil": {
      "name": "anvil",
      "version": "0.1.0",
      "owner": "testing"
    }
  },
  "tables": {
    "counts": {
      "input": {
        "sql": "\n        SELECT c.block_hash, c.tx_hash, c.address, c.block_num, c.timestamp, c.event['count'] as count\n        FROM (\n    SELECT block_hash, tx_hash, block_num, timestamp, address, evm_decode_log(topic1, topic2, topic3, data, 'Count(uint256 count)') as event\n    FROM anvil.logs\n    WHERE topic0 = evm_topic('Count(uint256 count)')\n  ) as c\n      "
      },
      "schema": {
        "arrow": {
          "fields": [
            {
              "name": "_block_num",
              "type": "UInt64",
              "nullable": false
            },
            {
              "name": "block_hash",
              "type": {
                "FixedSizeBinary": 32
              },
              "nullable": false
            },
            {
              "name": "tx_hash",
              "type": {
                "FixedSizeBinary": 32
              },
              "nullable": false
            },
            {
              "name": "address",
              "type": {
                "FixedSizeBinary": 20
              },
              "nullable": false
            },
            {
              "name": "block_num",
              "type": "UInt64",
              "nullable": false
            },
            {
              "name": "timestamp",
              "type": {
                "Timestamp": ["Nanosecond", "+00:00"]
              },
              "nullable": false
            },
            {
              "name": "count",
              "type": "Utf8",
              "nullable": true
            }
          ]
        }
      },
      "network": "anvil"
    },
    "transfers": {
      "input": {
        "sql": "\n        SELECT t.block_num, t.timestamp, t.event['from'] as from, t.event['to'] as to, t.event['value'] as value\n        FROM (\n    SELECT block_hash, tx_hash, block_num, timestamp, address, evm_decode_log(topic1, topic2, topic3, data, 'Transfer(address indexed from, address indexed to, uint256 value)') as event\n    FROM anvil.logs\n    WHERE topic0 = evm_topic('Transfer(address indexed from, address indexed to, uint256 value)')\n  ) as t\n      "
      },
      "schema": {
        "arrow": {
          "fields": [
            {
              "name": "_block_num",
              "type": "UInt64",
              "nullable": false
            },
            {
              "name": "block_num",
              "type": "UInt64",
              "nullable": false
            },
            {
              "name": "timestamp",
              "type": {
                "Timestamp": ["Nanosecond", "+00:00"]
              },
              "nullable": false
            },
            {
              "name": "from",
              "type": {
                "FixedSizeBinary": 20
              },
              "nullable": true
            },
            {
              "name": "to",
              "type": {
                "FixedSizeBinary": 20
              },
              "nullable": true
            },
            {
              "name": "value",
              "type": "Utf8",
              "nullable": true
            }
          ]
        }
      },
      "network": "anvil"
    }
  },
  "functions": {}
}
