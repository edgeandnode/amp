{
  "name": "@edgeandnode/amp-studio",
  "description": "UI for interacting with amp datasets. Writing, running and validating queries and datasets to create a dataset definition for deploying",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "start": "vite",
    "build": "vite build && tsc --noEmit",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "check": "prettier --write . && eslint --fix && tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "amp": "amp",
    "cli": "pnpm amp studio"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.4",
    "@connectrpc/connect": "^2.1.1",
    "@connectrpc/connect-web": "^2.1.1",
    "@edgeandnode/amp": "workspace:*",
    "@effect-atom/atom": "^0.4.2",
    "@effect-atom/atom-react": "^0.4.0",
    "@graphprotocol/gds-react": "https://pkg.pr.new/graphprotocol/gds/@graphprotocol/gds-react@b9ac299",
    "@monaco-editor/react": "4.8.0-rc.2",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-form": "^1.23.9",
    "@tanstack/react-query": "^5.90.8",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.135.2",
    "@tanstack/react-router-devtools": "^1.135.2",
    "@tanstack/router-plugin": "^1.135.2",
    "effect": "^3.19.3",
    "monaco-editor": "^0.54.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tanstack/eslint-config": "^0.3.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.8",
    "web-vitals": "^5.1.0"
  }
}
