{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "kind": {
      "description": "Dataset kind, must be `substreams`.",
      "type": "string"
    },
    "manifest": {
      "description": "Substreams package manifest URL.",
      "type": "string"
    },
    "module": {
      "description": "Substreams output module name.",
      "type": "string"
    },
    "name": {
      "description": "Dataset name.",
      "type": "string"
    },
    "network": {
      "description": "Network name, e.g., `mainnet`.",
      "type": "string"
    }
  },
  "type": "object"
}