{
  "name": "studio-cli",
  "version": "1.0.0",
  "description": "Temp cli tool for opening the local studio ui dataset query experience. this will soon be merged into the amp cli",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/edgeandnode/project-nozzle",
    "directory": "typescript/packages/studio-cli"
  },
  "bin": {
    "studio-cli": "./src/cli/bun.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts",
    "./cli/*": null,
    "./internal/*": null,
    "./*/index": null
  },
  "files": [
    "src/**/*.ts",
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map"
  ],
  "scripts": {
    "build": "tsc -b --force tsconfig.build.json",
    "studio-cli": "bun ./src/cli/bun.ts"
  },
  "dependencies": {
    "@effect/cli": "^0.69.2",
    "@effect/experimental": "^0.54.6",
    "@effect/platform-node": "^0.96.1",
    "jiti": "^2.5.1",
    "js-toml": "^1.0.2",
    "minimatch": "^10.0.3",
    "open": "^10.2.0",
    "viem": "^2.37.6"
  },
  "peerDependencies": {
    "@bufbuild/protobuf": "2.6.3",
    "@connectrpc/connect": "^2.1.0",
    "@connectrpc/connect-node": "^2.1.0",
    "@effect/experimental": "^0.54.6",
    "@effect/platform": "^0.90.10",
    "@effect/platform-node": "^0.96.1",
    "apache-arrow": "^21.0.0",
    "effect": "^3.17.13",
    "nozzl": "workspace:*"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.57.2",
    "@bufbuild/protoc-gen-es": "^2.9.0",
    "@connectrpc/connect": "^2.1.0",
    "@connectrpc/connect-node": "^2.1.0",
    "@effect/platform": "^0.90.10",
    "@effect/vitest": "^0.25.1",
    "@types/node": "^24.5.2",
    "apache-arrow": "^21.0.0",
    "effect": "^3.17.13",
    "nozzl": "workspace:*",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
