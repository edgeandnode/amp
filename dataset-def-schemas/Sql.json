{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "DatasetDef",
  "type": "object",
  "properties": {
    "kind": {
      "description": "Dataset kind, must be `sql`.",
      "type": "string"
    },
    "name": {
      "description": "Dataset name.",
      "type": "string"
    },
    "network": {
      "description": "Network name, e.g., `mainnet`.",
      "type": "string"
    }
  },
  "required": [
    "kind",
    "network",
    "name"
  ]
}