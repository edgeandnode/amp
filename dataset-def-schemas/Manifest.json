{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "dependencies": {
      "additionalProperties": {
        "properties": {
          "name": {
            "type": "string"
          },
          "owner": {
            "type": "string"
          },
          "version": {
            "description": "A semantic version requirement string, e.g., '>=1.0.0'.",
            "type": "string"
          }
        },
        "type": "object"
      },
      "type": "object"
    },
    "functions": {
      "additionalProperties": {
        "properties": {
          "input_types": {
            "items": {
              "description": "An Arrow data type, e.g., 'Int64', 'Utf8', etc.",
              "type": "string"
            },
            "type": "array"
          },
          "output_type": {
            "description": "An Arrow data type, e.g., 'Int64', 'Utf8', etc.",
            "type": "string"
          },
          "source": {
            "properties": {
              "filename": {
                "type": "string"
              },
              "source": {
                "type": "string"
              }
            },
            "type": "object"
          }
        },
        "type": "object"
      },
      "type": "object"
    },
    "kind": {
      "description": "Dataset kind, must be `manifest`.",
      "type": "string"
    },
    "name": {
      "description": "Dataset name.",
      "type": "string"
    },
    "network": {
      "description": "Network name, e.g., `mainnet`.",
      "type": "string"
    },
    "tables": {
      "additionalProperties": {
        "properties": {
          "input": {
            "description": "Table input, which can be a View.",
            "oneOf": [
              {
                "properties": {
                  "sql": {
                    "description": "SQL query defining the view.",
                    "type": "string"
                  }
                },
                "type": "object"
              }
            ],
            "type": "object"
          },
          "network": {
            "type": "string"
          },
          "schema": {
            "description": "Table schema, which is an Arrow Schema.",
            "properties": {
              "arrow": {
                "description": "Arrow schema of the table.",
                "properties": {
                  "fields": {
                    "description": "List of Fields in the schema.",
                    "items": {
                      "properties": {
                        "name": {
                          "description": "Field name.",
                          "type": "string"
                        },
                        "nullable": {
                          "description": "Boolean indicating whether the field is nullable.",
                          "type": "boolean"
                        },
                        "type_": {
                          "description": "Data type.",
                          "type": "string"
                        }
                      },
                      "type": "object"
                    },
                    "type": "array"
                  }
                },
                "type": "object"
              }
            },
            "type": "object"
          }
        },
        "type": "object"
      },
      "type": "object"
    },
    "version": {
      "description": "A semantic version string, e.g., '1.0.0'.",
      "type": "string"
    }
  },
  "type": "object"
}